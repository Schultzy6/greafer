{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Greafer (bot)",
            "request": "launch",
            "runtimeArgs": [
                "run",
                "start"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "outputCapture": "std"
        },
        {
            "name": "Deploy Greafer Commands",
            "request": "launch",
            "runtimeArgs": [
                "run",
                "deploy"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "outputCapture": "std"
        },
    ]
}